import pandas as pd
import re

df = pd.read_csv("linije.csv")
df = df.reindex(columns = df.columns[::-1])
df = df.dropna()

lista_stanica = str(df["Lista stanica"].values.tolist())

sve_koordinate = re.findall(r"\d+\.\d+", lista_stanica)

lista_koordinata = list({(float(sve_koordinate[i]), float(sve_koordinate[i+1])) for i in range(0, len(sve_koordinate), 2)})
